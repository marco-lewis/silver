def f0_oracle(const b:B)lifted:B{
    return b;
}

def f1_oracle(const b:B)lifted:B{
    return false;
}

def f2_oracle(const b:B)lifted:B{
    return true;
}

def f3_oracle(const b:B)lifted:B{
    return !b;
}

